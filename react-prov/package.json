{
  "name": "react-prov",
  "version": "1.0.0",
  "description": "React web application",
  "main": "webpack.config.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "build-dev": "powershell.exe -command \"$env:NODE_ENV='development'; ./node_modules/.bin/webpack\"",
    "build-pro": "powershell.exe -command \"$env:NODE_ENV='production'; ./node_modules/.bin/webpack\"",
    "start-server": "./node_modules/.bin/webpack-dev-server --content-base src --inline --hot",
    "start": "powershell.exe -command \"start http://localhost:8080; ./node_modules/.bin/webpack-dev-server --content-base app --inline --hot\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Chris Ryan",
  "license": "ISC",
  "dependencies": {
    "history": "^1.17.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.24.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "sass-loader": "^0.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
